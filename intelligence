import java.util.*;

public class intelligence {
    public static void main(String args[]){
        int y;
        double i,x;
        for(y=1;y<=6;y++){
            for(x=5.5;x<=12.5;x=x+0.5){
                i=2+(y+(0.5*x));
                System.out.println("APPROX VALUE OF INTELLIGENCE= "+i+"   "+x+"   "+y);
            }
        }
        
    }
    
}
