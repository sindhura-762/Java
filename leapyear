import java.util.*;
public class leapyear {
    public static void main(String args[]){
    int year ;
    try (Scanner sc = new Scanner(System.in)) {
        year = sc.nextInt();
    }
    if(year%4==0){
        System.out.println("ITS A LEAP YEAR");
    }
    else{
        System.out.println("ITS NOT A LEAP YEAR");
    }
    }
    
}
