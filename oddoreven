import java.util.Scanner;

public class oddoreven {
    public static void main(String args[]){
        int x;
        try (Scanner sc = new Scanner(System.in)) {
            x = sc.nextInt();
        }
        if(x%2==0){
            System.out.println("EVEN");
        }
            else{
                System.out.println("ODD");
            }
        }
    }
