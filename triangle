import java.util.*;
public class triangle {
    public static void main(String args[]){
        int a1,a2,a3,sum;
        try (Scanner sc = new Scanner(System.in)) {
            a1 = sc.nextInt();
            a2 = sc.nextInt();
            a3 = sc.nextInt();
        }
        sum=a1+a2+a3;
        if(sum==180){
            System.out.println("TRIANGLE");
        }
        else{
            System.out.println("NOT A TRIANGLE");
        }
    }
    
}
